{
  "type": "page",
  "remark": null,
  "body": [
    {
      "type": "crud",
      "name": "product list",
      "title": "产品列表 ${count}",
      "api": {
        "method": "post",
        "url": "http://192.168.118.168:6059/tacos-cloud-market/api/market/product/query",
        "data": {
          "code": "${code}",
          "resourceTypes": ["${keywords}"]
        }
      },
      "filter": {
        "title": "",
        "mode": "inline",
        "wrapWithPanel": false,
        "submitText": "",
        "controls": [
          {
            "type": "text",
            "name": "keywords",
            "required": true,
            "placeholder": "产品类型",
            "clearable": true
          },
          {
            "type": "text",
            "name": "code",
            "placeholder": "产品编码",
            "clearable": true
          },
          {
            "type": "submit",
            "label": "查询",
            "className": "btn-success"
          }
        ],
        "className": "m-b-sm"
      },
      "columns": [
        {
          "type": "static",
          "name": "name",
          "label": "name"
        },
        {
          "type": "static",
          "name": "code",
          "label": "code"
        },
        {
          "type": "static",
          "name": "version",
          "label": "version"
        },
        {
          "type": "static",
          "name": "typeCode",
          "label": "type"
        }
      ]
    }
  ]
}